{
  "usesTypeScript": false,
  "usesCssModule": false,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "customTemplates": {
        "component": "templates/component/component/TemplateName.jsx",
        "style": "templates/component/style/TemplateName.scss"
      },
      "path": "src/components",
      "withStyle": true,
      "withTest": true,
      "withStory": false,
      "withLazy": false
