    "page": {
      "customTemplates": {
        "component": "templates/component/component/TemplateName.jsx",
        "style": "templates/component/style/TemplateName.scss"
      },
      "path": "src/pages",
      "withStyle": true,
      "withTest": true,
      "withStory": false,
      "withLazy": false
    }
  }
}